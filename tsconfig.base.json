{
    "compilerOptions": {
      "outDir": "dist",
      "target": "es2018",
      "module": "esnext",
      "baseUrl": "./",
      "sourceMap": false,
      "moduleResolution": "node",
      "allowJs": true,
      "strict": true,
      "noUnusedLocals": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "removeComments": false,
      "rootDir": ".",
      "types": [],
      "paths": {
        "@/*": [
          "src/*"
        ],
      }
    }
  }
  